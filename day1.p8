#include input1.lua

function count_increases(arr)
 prev = 32767
 increases = 0
 for val in all(arr) do
  if type(val) == "number" then
	  if val > prev then
	   increases += 1
	  end
	  prev = val
	 end
	end
	return increases
end

arr = split(input, "\n", true)
increases = count_increases(arr)
print(increases)

