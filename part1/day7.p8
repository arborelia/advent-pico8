pico-8 cartridge // http://www.pico-8.com
version 34
__lua__
#include advent.p8
#include serial.p8

function insertion_sort(a)
 for i=1,#a do
  local j = i
  while j > 1 and a[j-1] > a[j] do
   a[j],a[j-1] = a[j-1],a[j]
   j = j - 1
  end
 end
end

-- get a median of the table
-- (smaller of the two if there
-- is an even number of items)
function median(tbl)
 insertion_sort(tbl)
 local mid=ceil(#tbl/2)
 return tbl[mid]
end

function crab_distance(tbl)
 cls(0)
 local median=median(tbl)
 local total=0
 for i=1,#tbl do
  x=tbl[i]
  line(x,i,median,i,2)
  total += minify(abs(x-median))
 end
 cursor(0,80)
 color(6)
 return inttext(total)
end

tosort = {0,4,2,0,6,9}
insertion_sort(tosort)
test(
 tosort,
 {0,0,2,4,6,9},
 "insertion sort"
)

crabs = {16,1,2,0,4,2,7,1,2,14}
test(
 median(crabs),
 2,
 "median of example"
)

test(
 crab_distance(crabs),
 "37",
 "example answer"
)

--split input on commas
input = split(
 read_lines()[1],
 ","
)
check(crab_distance(input))
