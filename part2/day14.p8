pico-8 cartridge // http://www.pico-8.com
version 34
__lua__
#include advent.p8

function color_code(letters)
 local counter=1
 local code={}
 for pos=1,#letters do
  l = at(letters,pos)
  if code[l]==nil then
   code[l] = counter
   counter += 1
  end
 end
 return code
end

-- parse rules of the form
-- "xy -> z" and the template
function parse_input(lines)
 local rules={}
 local start=nil
 for l in all(lines) do
  if #l == 7 then
   rules[sub(l,1,2)] = at(l,7)
  elseif #l > 0 then
   assert(start==nil)
   start=l
  end
 end
 return {rules=rules, start=start}
end

function polymerize(s, rules, code)
 local out=""
 for loc=1,#s-1 do
  local l1=at(s,loc)
  local l2=at(s,loc+1)
  local insert=rules[l1..l2]
  out ..= l1
  if insert != nil then
   out ..= insert
  end
 end
 out ..= at(s,#s)
 
 -- visualize the string
 for loc=1,#out do
  local c=code[at(out,loc)]
  local x=loc % 128
  local y=flr(loc/128)
  pset(x,y,c)
 end
 return out
end

function count_diff(letters)
 local counts={}
 for pos=1,#letters do
  local l=at(letters, pos)
  if counts[l] == nil then
   counts[l] = 1
  else
   counts[l] += 1
  end
 end
 local values={}
 for l,n in pairs(counts) do
  add(values, n)
 end
 insertion_sort(values)
 return values[#values] - values[1]
end

function simulate(input, steps)
 local polymer=input.start
 local rules=input.rules
 local code=color_code(input.start)
 
 cls(0)
 for step=1,steps do
  polymer = polymerize(polymer, rules, code)
 end
 cursor(0,100)
 
 return count_diff(polymer)
end

testcode = color_code("yeet")
test(testcode["y"], 1)
test(testcode["e"], 2)
test(testcode["t"], 3)

test(count_diff("yeet"), 1, "yeet")
test(count_diff("abracabam"), 3, "abraca-bam!")

input = parse_input(read_lines())
check(simulate(input, 10))
