pico-8 cartridge // http://www.pico-8.com
version 34
__lua__
#include advent.p8

function parse_folds(lines)
 local folds={}
 for l in all(lines) do
  if sub(l,1,10)=="fold along" then
   local fold=sub(l,12)
   local parts=split(fold,"=")
   add(folds, {dir=parts[1], pos=parts[2]})
  end
 end
 return folds
end

function parse_dots(lines)
 local dots={}
 for l in all(lines) do
  local parts=split(l, ",")
  if #parts==2 then
   add(dots, {x=parts[1], y=parts[2]})
  end
 end
 return dots
end

function apply_folds(dot, folds)
 local x=dot.x
 local y=dot.y
 for fold in all(folds) do
  if fold.dir=="x" then
   if x > fold.pos then
    x = fold.pos - (x-fold.pos)
   end
  elseif fold.dir=="y" then
   if y > fold.pos then
    y = fold.pos - (y-fold.pos)
   end
  else
   -- what the heck
   assert(false, fold.dir)
  end
 end
 return {x=x, y=y}
end

function count_dots(dots, folds)
 -- a table of coordinates like
 -- "3,2"
 cls(0)
 local seen={}
 local nseen=0
 for dot in all(dots) do
  local newdot = apply_folds(dot, folds)
  local coord = newdot.x ..",".. newdot.y
  if seen[coord] == nil then
   pset(newdot.x, newdot.y, 10)
   seen[coord]=true
   nseen += 1
  end
 end
 cursor(0,100)
 return nseen
end

input = read_lines()
dots = parse_dots(input)
folds = parse_folds(input)
check(count_dots(dots,folds), 17)

