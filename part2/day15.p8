__lua__
#include advent.p8

scale = {
 [0]=0, 1, 2, 5, 3,
 8, 14, 9, 10, 15, 7
}

function show_grid(tbl)
 if (#tbl > 10) return 
 for row=1,10 do
  for col=1,10 do
   local val=tbl[row][col]
   rectfill(
    col*10, row*10,
    col*10+9, row*10+9,
    scale[val]
   )
  end
 end
end

function draw_path(r1,c1,r2,c2)
 line(
  c1*10+5, r1*10+5,
  c2*10+5, r2*10+5,
  7
 )
end

function draw_paths(pointers)
 for row=1,10 do
  for col=1,10 do
   local ptr=pointers[row][col]
   if ptr != nil then
    local r2=ptr[1]
    local c2=ptr[2]
    draw_path(row,col,r2,c2)
   end
  end
 end
end

function draw_costs(costs)
 for row=1,10 do
  for col=1,10 do
   local cost=costs[row][col]
   if cost < 9999 then
    cursor(col*10, row*10)
    color(6)
    print(cost)
   end
  end
 end
end

function parse_grid(lines)
 local grid={}
 for l in all(lines) do
  local row=split(l,"")
  add(grid, row)
 end
 return grid
end

function find_path(grid)
 -- start all the best known
 -- costs as 9999, a number
 -- which is hopefully bigger
 -- than the biggest cost
 costs={}
 pointers={}
 local nrows = #grid
 local ncols = #grid[1]
 for row=1,nrows do
  local crow={}
  local prow={}
  for col=1,ncols do
   add(crow,9999)
   add(prow,nil)
  end
  add(costs,crow)
  add(pointers,prow)
 end
 costs[1][1] = 0
 for step=1,nrows * ncols do
  local updated=false
  for row=1,nrows do
   for col=1,ncols do
    local ccost=grid[row][col]
    for dr=-1,1 do
     for dc=-1,1 do
      if abs(dr)+abs(dc)==1 then
       if grid[row+dr] != nil then
        if grid[col+dc] != nil then
         local best=costs[row][col]
         local cost=ccost + costs[row+dr][col+dc]
         if cost < best then
          costs[row][col] = cost
          pointers[row][col] = {row+dr,col+dc}
          updated=true
         end
        end
       end
      end
     end
    end
   end
  end
  
  if #grid <= 10 then
   cls(4)
   show_grid(grid)
   draw_paths(pointers)
   draw_costs(costs)
  else
   print(costs[nrows][ncols])
  end
  if (not updated) break
  flip()
 end
 cursor(0,110)
 return costs[nrows][ncols]
end

